<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.CreateConcertController">
   <children>
      <TextField fx:id="artistNameValue" alignment="TOP_LEFT" layoutX="25.0" layoutY="125.0" prefHeight="25.0" prefWidth="120.0" promptText="Artist/band name" />
      <Label layoutX="26.0" layoutY="106.0" prefHeight="17.0" prefWidth="291.0" text="Artist, scene Cost(int),concertdate, concertID" />
      <TextField fx:id="costValue" alignment="TOP_LEFT" layoutX="26.0" layoutY="187.0" prefHeight="25.0" prefWidth="120.0" promptText="10000" />
      <TextField fx:id="concertDateValue" alignment="TOP_LEFT" layoutX="26.0" layoutY="218.0" prefHeight="25.0" prefWidth="120.0" promptText="2020-01-01" />
      <TextField fx:id="concertIdValue" alignment="TOP_LEFT" layoutX="26.0" layoutY="248.0" prefHeight="25.0" prefWidth="120.0" promptText="ABCDEFFGHT" />
      <TextField fx:id="sceneValue" alignment="TOP_LEFT" layoutX="26.0" layoutY="156.0" prefHeight="25.0" prefWidth="120.0" promptText="Ullevi" />
      <Button fx:id="createConcertButton" layoutX="34.0" layoutY="286.0" mnemonicParsing="false" onAction="#createConcert" text="Create concert" />
   </children>
</AnchorPane>
