      <TextField fx:id="placesValues" layoutX="16.0" layoutY="38.0" prefHeight="47.0" prefWidth="291.0" promptText="Venue, city,Contrycode,renomme, amount of tickets" />
      <TextField fx:id="cancelConcertValue" layoutX="16.0" layoutY="258.0" prefHeight="25.0" prefWidth="194.0" promptText="Cancel concert" />
