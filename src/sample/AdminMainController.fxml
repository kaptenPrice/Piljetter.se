      <Label layoutX="398.0" layoutY="130.0" text="Amount of sold tickets" />
      <Label layoutX="453.0" layoutY="38.0" text="Rapports" />
      <Label layoutX="398.0" layoutY="169.0" text="Profitability" />
      <Button fx:id="profitabilityButton" layoutX="539.0" layoutY="165.0" mnemonicParsing="false" onAction="#profitability" text="Create report" />
      <TextField alignment="TOP_LEFT" layoutX="355.0" layoutY="215.0" prefHeight="316.0" prefWidth="272.0" promptText="Your report will appear here" />
   </children>
</fx:root>
